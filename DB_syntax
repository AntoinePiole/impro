
user : {
    id: id, //In the database, it is _id, which is given by mongoose, and is thus not shown in the model
    email: String,
    password: String,
    phone: String,
    familyName: String,
    firstName: String,
    username: String,
    birthday: Date,
    creationDate: Date,
    photoId: id
}

league : {
    id: id, //In the database, it is _id, which is given by mongoose, and is thus not shown in the model
    name: String,
    nickname: String,
    desc: String,
    mail: String,
    photoId: id,
    members: [{id:String,isAdmin:boolean}],  
    receivedMatchRequestIds: [String],
    sentMatchRequestIds: [String]s

}


match : {
    id: id, //In the database, it is _id, which is given by mongoose, and is thus not shown in the model
    name: String,
    status: String,  //between [finished, waitingConfirmation, confirmed, canceled]
    league1Id: id,
    league2Id: id,
    league1Members: [String],
    league2Members: [String],
    league1MembersPropositions: [String],
    league2MembersPropositions: [String],
    referee: id,
    mc: id,
    refereePropositions: id[],
    mcPropositions: id[],
    admins: id[],
    subscribers: id[],
    description: String,
    date: Date,
    location: String,
    score: (int,int)
}

